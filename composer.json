{
    "name": "allocine/graph-client-php",
    "description": "A basical php library for GraphQl HTTP calls",
    "type": "library",
    "autoload": {
        "psr-4": {
            "GraphClientPhp\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Test\\": "tests/"
        }
    },
    "config": {
        "bin-dir": "bin",
        "sort-packages": true
    },
    "require": {
        "php": ">=7.1",
        "guzzlehttp/guzzle": "^6.3",
        "symfony/cache": "^4.1",
        "symfony/filesystem": "^4.1",
        "symfony/finder": "^4.1",
        "webonyx/graphql-php": "^0.12.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.2",
        "phpmd/phpmd": "^2.6",
        "sebastian/phpcpd": "^4.0",
        "squizlabs/php_codesniffer": "^3.2"
    },
    "minimum-stability": "stable",
    "license": "MIT",
    "authors": [
        {
            "name": "Allocine DEVELOPERS",
            "email": "developers@allocine.fr"
        }
    ]
}
